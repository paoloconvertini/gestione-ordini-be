	<queryString language="SQL">
		<![CDATA[SELECT *, 0 as valoreTotale
FROM ORDCLI2 c2
inner join ORDCLI c ON c.anno = c2.anno AND c.serie = c2.serie
AND c.progressivo = c2.progressivo
left join PIANOCONTI pc ON pc.gruppoconto = c.gruppocliente
left join FIRMAORDINECLIENTE f ON c.anno = f.anno AND c.serie = f.serie
AND c.progressivo = f.progressivo
AND c.contocliente = pc.sottoconto]]>
	</queryString>
